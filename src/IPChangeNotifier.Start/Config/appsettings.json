{
  "jobConfig": {
    "requestInterval": 60
  },
  "messageSender": {
    "channelName": "TestChannelName",
    "url": "http://<host>:<port>/api/Message/SendMessageAsync"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Graylog"
    ],
    "MinimumLevel": {
      "Default": "Verbose"      
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "restrictedToMinimumLevel": "Debug"
        }
      },
      {
        "Name": "Graylog",
        "Args": {
          "hostnameOrAddress": "localhost",
          "port": "12201",
          "transportType": "Udp",
          "facility": "IPNotifier"
        }
      }
    ]    
  }
}
